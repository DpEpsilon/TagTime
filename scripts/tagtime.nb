  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"s", ",", "e"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"s", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"NumberQ", "[", "start", "]"}], "||", 
         RowBox[{"start", "===", "Infinity"}]}], ",", "start", ",", 
        RowBox[{"tm", "@", "start"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"e", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"NumberQ", "[", "end", "]"}], "||", 
         RowBox[{"end", "===", "Infinity"}]}], ",", "end", ",", 
        RowBox[{"tm", "@", "end"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Last", "/@", 
      RowBox[{"implicitZeroify", "@", 
       RowBox[{"floorify", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"tm", "@", 
